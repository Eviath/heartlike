<tr>


  <% resource.attributes.each do |key, value| %>
    <td><%= render_resource_attribute(key, value) %></td>
  <% end %>


  <% resource.association_attributes.each do |key, value| %>
    <td><%= render_resource_attribute(key, value) %></td>
  <% end %>


  <th><%= link_to t('heartlike.actions.show'), admin_resource_path(resource.attributes['id'], resource: resource.resource) %>
    - <%= link_to t('heartlike.actions.edit'), edit_admin_resource_path(resource.attributes['id'], resource: resource.resource) %>
    - <%= link_to t('heartlike.actions.destroy'), admin_resource_path(resource.attributes['id'], resource: resource.resource), method: :delete %> </th>
</tr>

