<!DOCTYPE html>
<html>
<head>
  <title>Heartlike - Dashboard</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'heartlike/application', 'data-turbolinks-track': 'reload' %>
</head>

<body>

<div class="dashboard_row">
  <div class="dashboard_nav">
    <%= render 'heartlike/admin/dashboard/dashboard_nav' %>
  </div>
  <div class="dashboard_content">
    <%= yield   %>
  </div>
</div>

</body>
</html>


